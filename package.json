{
    "name": "invo-sdk",
    "version": "1.0.0",
    "type": "module",
    "description": "INVO SDK Typescript",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "prepare": "npm run build",
        "build": "rollup -c",
        "build:watch": "tsc --watch",
        "clean": "rm -rf dist",
        "test:invoice": "npx tsx test/test-invoice.ts",
        "test:makeup": "npx tsx test/test-makeup.ts",
        "test:reader": "npx tsx test/test-reader.ts",
        "test:token": "npx tsx test/test-api_key.ts",
        "prepublishOnly": "npm run clean && npm run build",
        "types:sandbox": "npx tsx ./swagger.ts sandbox",
        "types": "npx tsx ./swagger.ts"
    },
    "keywords": [
        "auth",
        "authentication",
        "jwt",
        "oauth",
        "typescript",
        "sdk",
        "calltek"
    ],
    "author": "Calltek",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/calltek/invo-sdk-ts.git"
    },
    "bugs": {
        "url": "https://github.com/calltek/invo-sdk-ts/issues"
    },
    "homepage": "https://github.com/calltek/invo-sdk-ts#readme",
    "devDependencies": {
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "rollup": "^4.53.3",
        "rollup-plugin-dts": "^6.2.3",
        "swagger-typescript-api": "^13.2.16",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
